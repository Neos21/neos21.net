{
  "name": "neo.s21.xrea.com",
  "description": "Neo's World",
  "private": true,
  "scripts": {
    "build": "npm run all && npm run feeds && npm run sitemap",
    "prebuild": "npm run clear",
    "clear": "node ./bin/clear-dist.js",
    "all": "node ./bin/build-all.js && npm run css",
    "css": "cleancss --format beautify --output ./dist/styles.css ./src/styles/styles.css",
    "feeds": "node ./bin/build-feeds.js",
    "sitemap": "node ./bin/build-sitemap.js",
    "dev": "node ./bin/dev-server.js",
    "file": "node ./bin/build-files.js",
    "serve": "serve --listen 3000 ./dist/",
    "upload-all-pages": "node ./bin/upload-all-pages.js",
    "up": "node ./bin/upload-files.js"
  },
  "author": "Neo <neos21@gmail.com> (https://neos21.net/)",
  "license": "MIT",
  "homepage": "https://neos21.net/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Neos21/neo.s21.xrea.com.git"
  },
  "bugs": {
    "url": "https://github.com/Neos21/neo.s21.xrea.com/issues"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/Neos21"
  },
  "devDependencies": {
    "@mapbox/rehype-prism": "0.5.0",
    "@neos21/neos-normalize": "2.0.0",
    "browser-sync": "2.26.13",
    "clean-css-cli": "4.3.0",
    "ftp-client": "0.2.2",
    "rehype-autolink-headings": "4.0.0",
    "rehype-document": "5.1.0",
    "rehype-format": "3.1.0",
    "rehype-parse": "7.0.1",
    "rehype-slug": "3.0.0",
    "rehype-stringify": "8.0.0",
    "rehype-toc": "3.0.2",
    "remark-extract-frontmatter": "2.0.3",
    "remark-frontmatter": "2.0.0",
    "remark-parse": "8.0.3",
    "remark-rehype": "8.0.0",
    "remark-slug": "6.0.0",
    "remark-toc": "7.0.0",
    "serve": "11.3.2",
    "unified": "9.2.0",
    "yaml": "1.10.0"
  }
}
