{
  "name": "neos21.net",
  "description": "Neo's World",
  "private": true,
  "scripts": {
    "?-bin": "echo './bin/ Scripts' : GitHub Actions Uses build-feeds'ãƒ»'build-sitemap'",
    "build-all": "node ./bin/build-all.js",
    "build-feeds": "node ./bin/build-feeds.js",
    "build-files": "node ./bin/build-files.js",
    "build-sitemap": "node ./bin/build-sitemap.js",
    "create-blog-indexes": "node ./bin/create-blog-indexes.js",
    "create-blog-post-boilerplate": "node ./bin/create-blog-post-boilerplate.js",
    "clear-dist": "node ./bin/clear-dist.js",
    "detect-change-files": "node ./bin/detect-change-files.js",
    "dev-server": "node ./bin/dev-server.js",
    "upload-all-pages": "node ./bin/upload-all-pages.js",
    "upload-files": "node ./bin/upload-files.js",
    "?-css": "echo 'CSS Build' : GitHub Actions Uses 'build-css'",
    "build-css": "cleancss --format beautify --output ./dist/styles.css ./src/styles/styles.css",
    "?-aliases": "echo 'Aliases'",
    "all": "npm run build-all && npm run build-css",
    "build": "npm run clear-dist && npm run build-all && npm run build-css && npm run build-feeds && npm run build-sitemap",
    "rebuild": "npm run build-all && npm run build-css && npm run build-feeds && npm run build-sitemap",
    "b": "npm run create-blog-post-boilerplate",
    "b-index": "npm run create-blog-indexes",
    "clear": "npm run clear-dist",
    "css": "npm run build-css",
    "dev": "npm run dev-server",
    "f": "npm run build-files",
    "feed": "npm run build-feeds",
    "sitemap": "npm run build-sitemap",
    "start": "npm run serve",
    "u": "npm run upload-files",
    "?-etc": "echo 'Etc.'",
    "serve": "sirv ./dist",
    "blc": "broken-link-checker --exclude-external --ordered --recursive http://localhost:5000/"
  },
  "author": "Neo <neos21@gmail.com> (https://neos21.net/)",
