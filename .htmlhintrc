{
  // Alt 属性
  "alt-require": true,
  // 属性は小文字で
  "attr-lowercase": true,
  // 属性の重複チェック
  "attr-no-duplication": true,
  // アンセーフな文字チェック
  "attr-unsafe-chars": true,
  // 属性値はダブルクォートで囲む
  "attr-value-double-quotes": true,
  // 属性値に空値を許容しない : async 属性が引っかかるので無視
  "attr-value-not-empty": false,
  // DOCTYPE 宣言
  "doctype-first": true,
  // HTML5 の DOCTYPE 宣言
  "doctype-html5": true,
  // head 要素内にスクリプト禁止 : 置いているので無視
  "head-script-disabled": false,
  // リンクは相対パス ('abs' Absolute) か絶対パス ('rel' Relative) : 無視
  "href-abs-or-rel": false,
  // AdBlock に該当する ID・クラス名禁止
  "id-class-ad-disabled": true,
  // ハイフンケースの ID・Class
  "id-class-value": "dash",
  // ID がユニークであること
  "id-unique": true,
  // インラインスクリプト禁止 : #xrea の画像に指定しているので無視
  "inline-script-disabled": false,
  // インラインスタイル禁止 : AdSense コードが使うので無視
  "inline-style-disabled": false,
  // タブ混在禁止・スペースのみ
  "space-tab-mixed-disabled": "space",
  // 実体参照文字をエスケープしていること
  "spec-char-escape": true,
  // 空の src 属性禁止
  "src-not-empty": true,
  // style 要素禁止 : 使用箇所があるので無視
  "style-disabled": false,
  // 要素が正しく閉じられていること
  "tag-pair": true,
  // 空要素に閉じタグ強制 : 閉じタグを付けないので無視
  "tag-self-close": false,
  // 要素名は小文字
  "tagname-lowercase": true,
  // title 要素指定
  "title-require": true
}
